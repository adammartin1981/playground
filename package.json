{
  "name": "playground",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleDirectories": [
      "node_modules",
      "test"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react-dom": "^17.0.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.15",
    "@types/react": "^17.0.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "react": "^17.0.1",
    "ts-jest": "^26.4.4"
  }
}
